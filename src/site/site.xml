    <custom>
        <reflowSkin>
            <!-- Make this to 'false' for local development, i.e. file:// URLs -->
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>true</smoothScroll>
            <theme>bootswatch-cerulean</theme>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <absoluteResourceURL>http://andriusvelykis.github.com/reflow-maven-skin/</absoluteResourceURL>
            <brand>
                <!-- Brand text in top-left part of the site -->
                <name><![CDATA[
                <img src="images/logo.png" border="0" alt="MapDB"/> - java beyond heap
                ]]></name>
                <href>http://www.mapdb.org/</href>
            </brand>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <toc>top</toc>

            <!-- Include the documentation and tools in the top navigation (in addition to links) -->
            <topNav>MapDB|FAQ|Documentation|Support</topNav>
            <!-- Split menus in the bottom navigation -->
            <bottomNav>
                <!--number of cols must be 4 -->
                <column>MapDB</column>
                <column>FAQ</column>
                <column>Documentation</column>
                <column>Support|Author</column>
            </bottomNav>
            <bottomDescription>
                <![CDATA[
<span class="color-highlight">MapDB</span> provides concurrent Maps, Sets and Queues backed by disk storage or off-heap memory.
It is a fast and easy to use embedded Java database engine. And it is completely free under Apache License.
MapDB is free as speech and free as beer under <a href="https://github.com/jankotek/MapDB/blob/master/license.txt">Apache License 2.0</a>.
]]>
            </bottomDescription>
            <pages>
                <index project="mapdb">
                    <!-- Override the title -->
                    <titleTemplate>MapDB </titleTemplate>
                    <!-- no breadcrumbs on the main index page -->
                    <breadcrumbs>false</breadcrumbs>
                    <!-- no ToC on the main index page -->
                    <toc>false</toc>
                    <markPageHeader>false</markPageHeader>
                    <sections>
                        <carousel />
                        <body />
                        <sidebar />
                    </sections>
                </index>
                <faq-general>
                    <toc>sidebar</toc>
                </faq-general>
                <faq-problems>
                    <toc>sidebar</toc>
                </faq-problems>
                <faq-performance>
                    <toc>sidebar</toc>
                </faq-performance>
                <changelog>
                    <toc>sidebar</toc>
                </changelog>

            </pages>
        </reflowSkin>
    </custom>


